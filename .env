# Project Configuration
PROJECT_ID=your-project-id
TPU_ZONE=us-east1-c
ACCELERATOR_TYPE=v5litepod-4
TPU_NAME=your-tpu-name
TPU_NETWORK=tpu-network
TPU_SUBNET=tpu-subnet

# Runtime Configuration
RUNTIME_VERSION=tpu-vm-tf-2.15.0-pjrt
USE_PYTORCH_XLA=false

# Storage Configuration
BUCKET_NAME=your-bucket-name
CHECKPOINT_DIR=checkpoints
DATA_DIR=data
CACHE_DIR=cache
LOG_DIR=logs

# Resource Management
QUEUED_RESOURCE_ID=my-tpu-resource
QUOTA_TYPE=on-demand
IMAGE_NAME=my-tpu-training-image

# Training Configuration
MAX_SEQUENCE_LENGTH=512

# Monitoring Configuration
WANDB_PROJECT=transformer-ablation
TENSORBOARD_LOG_DIR=logs/tensorboard
GOOGLE_APPLICATION_CREDENTIALS="/path/to/service-account-key.json" 